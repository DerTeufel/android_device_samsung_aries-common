# mount point	fstype		device				options (length for cryptfs encryption footer)

/recovery	mtd		recovery
/boot		mtd		boot
/radio		mtd		radio

/data		ext4		/.secondrom/.secondrom/data.img    length=-16384
#/data		ext4		/.secondrom/.secondrom/data.img		loop	
/cache		yaffs2		cache

/sdcard		vfat		/dev/block/mmcblk0p1
/system		ext4		/dev/lvpool/secondary_system

/external_sd		vfat		/dev/block/mmcblk1p1
