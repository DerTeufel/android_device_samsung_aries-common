# mount point	fstype		device				options (length for cryptfs encryption footer)

/recovery	mtd		recovery
/boot		mtd		boot
/radio		mtd		radio

/data		ext4		/.secondrom/.secondrom/data.img    length=-16384
/cache		yaffs2		cache

/sdcard		vfat		/dev/block/mmcblk0p1    lun=/sys/devices/platform/s3c-usbgadget/gadget/lun0/file
/system		ext4		/dev/lvpool/secondary_system

/external_sd		vfat		/dev/block/mmcblk1p1    lun=/sys/devices/platform/s3c-usbgadget/gadget/lun1
